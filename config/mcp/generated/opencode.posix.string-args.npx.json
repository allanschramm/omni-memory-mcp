{
  "mcp": {
    "omni-memory": {
      "type": "local",
      "command": "npx",
      "args": [
        "-y",
        "@sharkdyt/omni-memory-mcp"
      ],
      "environment": {
        "OMNI_MEMORY_DIR": "/home/your-user/.omni-memory"
      },
      "enabled": true,
      "timeout": 5000
    }
  }
}
